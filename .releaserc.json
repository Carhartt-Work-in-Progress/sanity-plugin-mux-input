{
  "branches": [
    {"name": "studio-v2", "channel": "studio-v2"},
    {"name": "main", "channel": "studio-v3", "prerelease": true}
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/npm",
      {
        "tarballDir": "release"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": "release/*.tgz"
      }
    ]
  ],
  "preset": "angular"
}
